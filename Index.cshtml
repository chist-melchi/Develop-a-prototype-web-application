@model IEnumerable<Article>
@{
    ViewData["Title"] = "Sustainable Farming Articles";
}

<h2>@ViewData["Title"]</h2>

<ul>
    @foreach (var article in Model)
    {
        <li>
            <a href="/SustainableFarming/Details/@article.Id">@article.Title</a>
            <p>@article.Content</p>
        </li>
    }
</ul>
